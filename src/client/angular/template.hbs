//检查angularjs是否存在
if(typeof angular === 'object'){
    //angular js模块声明
    var __eazyajaxModule = angular.module('eazyajax',[]);

    {{#each moduleStubs as |module|}}
        __eazyajaxModule.service('{{module.pascalcaseName}}Service',function($q){
            {{#each module.methods as |method|}}
            this.{{method.name}} = function(){
                var args = Array.prototype.slice.call(arguments);
                return $q(function(resolve,reject){
                    __eazyajax_invoker('{{module.name}}','{{method.name}}',args)
                        .then(function(returnValue){
                            resolve(returnValue);
                        })
                        .catch(function(error){
                            reject(error);
                        });
                });
            };
            {{/each}}
        });
    {{/each}}
}else{
    console.warn('[EazyAjax] AngularJS不存在,所以没有挂载AngularJS调用的Services');
}